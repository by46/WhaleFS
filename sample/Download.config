<?xml version="1.0" encoding="utf-8" ?>
<DownloadConfig>
  <DownloadAppConfigs>
    <!--mall这个配置暂时商城在用，如果订单易的要调整的话，需要找商城确认-->
    <DownloadAppConfig Name="mall">
      <!--0:Pic,1:Text-->
      <WatermarkType>Text</WatermarkType>
      <!--水印地址，相对于网站根目录-->
      <WatermarkPic>watermark.png</WatermarkPic>
      <WatermarkText>
        <Text>用户自传证书</Text>
        <!--字体大小-->
        <FontSize>10</FontSize>
        <!--字体颜色-->
        <FontColorA>100</FontColorA>
        <FontColorR>0</FontColorR>
        <FontColorG>0</FontColorG>
        <FontColorB>0</FontColorB>
        <!--字体-->
        <FontFamily>宋体</FontFamily>
        <!--字体样式，Regular，Bold，Italic，Underline，Strikeout-->
        <FontStyle>Regular</FontStyle>
        <!--字体位置,LeftTop,LeftMiddle,LeftBottom,CenterTop,Center,CenterBottom,RightTop,RightMiddle,RightBottom-->
        <FontPosition>Center</FontPosition>
        <!--字体大小是否适应图片大小,如果true，则FontSize不会生效-->
        <FontAdaptation>true</FontAdaptation>
        <!--字体按比例缩放(FontAdaptation为true的话，可以适应图片的大小的比例)-->
        <FontScale>1</FontScale>
      </WatermarkText>
    </DownloadAppConfig>
