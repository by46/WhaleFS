    <!--商品图片上传,其中RootPath定死为neg不能做修改，程序中有些地方需要用它来计算-->
    <UploadAppConfig Name="product" RootPath="pdt">
      <!--水印地址，相对于网站根目录-->
      <WatermarkPic>watermark.png</WatermarkPic>
